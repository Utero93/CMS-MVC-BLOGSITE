<!-- Loop over each object in the projects array -->

<div class="card-container">

{{#each posts as |post| }}

  <div class="card post-card">
    <h2>
     <a class="post-titles" href="/post/{{post.id}}">{{post.title}}</a>
    </h2>
    <p class="post-by">
      By {{post.user.username}} on {{format_date post.date_created}}
    </p>
    
  </div>

{{/each}}

</div>


